{
  "name": "ilojo-bar",
  "version": "1.0.0",
  "description": "Ilojo Bar",
  "main": "app.js",
  "type": "module",
  "engines": {
    "node": "15.7.0"
  },
  "directories": {
    "doc": "docs"
  },
  "dependencies": {
    "@prismicio/client": "^6.5.0",
    "@prismicio/helpers": "^2.3.0",
    "compression": "^1.7.4",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.8",
    "express": "^4.17.3",
    "express-ejs-layouts": "^2.5.1",
    "gsap": "^3.10.4",
    "node": "^17.7.1",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@node-minify/cli": "^6.2.0",
    "@node-minify/uglify-js": "^6.2.0",
    "@types/three": "^0.130.0",
    "clean-css-cli": "^5.6.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "three": "^0.130.1",
    "ts-loader": "^9.2.5"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "node app.js",
    "build": "npm-run-all build:*",
    "build:css": "cleancss -o public/css/style.css src/css/style-src.css",
    "build:jsDiscover": "node-minify --compressor uglify-js --input 'src/js/discover-src.js' --output 'public/js/discover.js'",
    "build:jsMainScrollTrigger": "node-minify --compressor uglify-js --input 'src/js/mainScrollTrigger-src.js' --output 'public/js/mainScrollTrigger.js'",
    "build:jsNavigation": "node-minify --compressor uglify-js --input 'src/js/navigation-src.js' --output 'public/js/navigation.js'",
    "build:jsScrollingStory": "node-minify --compressor uglify-js --input 'src/js/scrollingStory-src.js' --output 'public/js/scrollingStory.js'",
    "build:jsServiceWorker": "node-minify --compressor uglify-js --input 'src/serviceWorker-src.js' --output 'public/serviceWorker.js'",
    "test": "nodemon app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JeanyDeVries/ilojo-bar.git"
  },
  "keywords": [
    "Ilojo bar",
    "Legacy1995"
  ],
  "author": "Jorn veltrop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JeanyDeVries/ilojo-bar/issues"
  },
  "homepage": "https://github.com/JeanyDeVries/ilojo-bar"
}

